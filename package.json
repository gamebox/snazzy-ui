{
  "version": "0.1.3",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "node ./scripts/build.mjs",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "node ./scripts/build.mjs",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@snazzyui/snazzyui",
  "author": "Anthony Bullard",
  "module": "dist/snazzy-ui.esm.js",
  "size-limit": [
    {
      "path": "dist/snazzy-ui.cjs.production.min.js",
      "limit": "5.19 KB"
    },
    {
      "path": "dist/snazzy-ui.esm.js",
      "limit": "3.15 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.4",
    "esbuild": "^0.14.5",
    "husky": "^7.0.4",
    "size-limit": "^7.0.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "snabbdom": "^3.1.0"
  }
}
